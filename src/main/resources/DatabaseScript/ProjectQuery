CREATE SCHEMA if not exists 'javaisawesome' ;

CREATE TABLE IF NOT EXISTS `javaisawesome`.`USER` (
	`userid` VARCHAR(50) NOT NULL,
	`username` VARCHAR(50) NOT NULL,
	`email` VARCHAR(50) NOT NULL UNIQUE,
	`password` VARCHAR(50) NOT NULL,
	PRIMARY KEY (`userid`)
);
  
INSERT INTO `javaisawesome`.`user` (`userid`,`username`,`email`, `password`) VALUES ('prince','prince kumar','Pk@12345', '12345');
  
  